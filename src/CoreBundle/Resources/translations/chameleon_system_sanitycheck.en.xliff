      <trans-unit id="check.bundles_loaded.directorynotopened">
        <source>check.bundles_loaded.directorynotopened</source>
        <target>Directory could not be opened: %0%</target>
      </trans-unit>
      <trans-unit id="check.bundles_loaded.notregistered">
        <source>check.bundles_loaded.notregistered</source>
        <target>Bundle is not registered: %0%</target>
      </trans-unit>
      <trans-unit id="check.bundles_loaded.ok">
        <source>check.bundles_loaded.ok</source>
        <target>All bundles are registered.</target>
      </trans-unit>
      <trans-unit id="check.cache.active">
        <source>check.cache.active</source>
        <target>Cache is active.</target>
      </trans-unit>
      <trans-unit id="check.cache.activedev">
        <source>check.cache.activedev</source>
        <target>Cache is active. This might be obstructive in development mode.</target>
      </trans-unit>
      <trans-unit id="check.cache.inactivedev">
        <source>check.cache.inactivedev</source>
        <target>Cache is inactive. This is ok in development mode.</target>
      </trans-unit>
      <trans-unit id="check.cache.notactive">
        <source>check.cache.notactive</source>
        <target>Cache is inactive. For better performance please configure caching.</target>
      </trans-unit>
      <trans-unit id="check.database_utf8check.ok">
        <source>check.database_utf8check.ok</source>
        <target>Check of used character set for the database connection was successful.</target>
      </trans-unit>
      <trans-unit id="check.database_utf8check.wrongcharset">
        <source>check.database_utf8check.wrongcharset</source>
        <target>Wrong Character Set: '%0%' uses Character Set '%1%' instead of 'utf8'</target>
      </trans-unit>
      <trans-unit id="check.database_utf8check.wrongcollation">
        <source>check.database_utf8check.wrongcollation</source>
        <target>Wrong Collation: '%0%' uses Collation '%1%' instead of 'utf8_unicode_ci'</target>
      </trans-unit>
      <trans-unit id="check.cronjob.toolong">
        <source>check.cronjob.toolong</source>
        <target>Cronjobs have been triggered %0% seconds ago. The threshold is set to %1% seconds.</target>
      </trans-unit>
      <trans-unit id="check.cronjob.ok">
        <source>check.cronjob.ok</source>
        <target>Cronjobs are being triggered correctly</target>
      </trans-unit>
      <trans-unit id="check.cronjob.never">
        <source>check.cronjob.never</source>
        <target>Cronjobs aren't being triggered.</target>
      </trans-unit>
      <trans-unit id="check.imagemagick.found">
        <source>check.imagemagick.found</source>
        <target>ImageMagick was found.</target>
      </trans-unit>
      <trans-unit id="check.imagemagick.inactive">
        <source>check.imagemagick.inactive</source>
        <target>ImageMagick is deactivated.</target>
      </trans-unit>
      <trans-unit id="check.imagemagick.notfound">
        <source>check.imagemagick.notfound</source>
        <target>ImageMagick was not found and can therefore not be used.</target>
      </trans-unit>
      <trans-unit id="label.directorieswritable">
        <source>label.directorieswritable</source>
        <target>Needed write permissions on directories given.</target>
      </trans-unit>
      <trans-unit id="label.diskspace">
        <source>label.diskspace</source>
        <target>Enough disk space is available.</target>
      </trans-unit>
