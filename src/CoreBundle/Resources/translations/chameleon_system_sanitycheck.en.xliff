      </trans-unit>
      <trans-unit id="label.supportedphpversion">
        <source>label.supportedphpversion</source>
        <target>A supported PHP version is used.</target>
      </trans-unit>
      <trans-unit id="label.databaseutf8">
        <source>label.databaseutf8</source>
        <target>The database connection uses UTF-8 encoding.</target>
      </trans-unit>
